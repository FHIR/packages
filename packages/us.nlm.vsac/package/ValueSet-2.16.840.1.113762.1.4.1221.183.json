  "version" : "20240423",
  "name" : "ClomipheneSelectiveEstrogenReceptorModulatorSERM",
  "title" : "Clomiphene (Selective Estrogen Receptor Modulator / SERM)",
  "status" : "active",
  "date" : "2024-04-23T01:01:49-04:00",
  "publisher" : "NACHC Steward",
  "description" : "This value set clarifies that clomiphene is being used as a SERM for a specific purpose (infertility) and not for its other potential applications.",
  "jurisdiction" : [
    {
      "coding" : [
        {
