/// url = 'http://hl7.org/fhir/StructureMap/BiologicallyDerivedProductDispense5to4B'
/// name = 'BiologicallyDerivedProductDispense5to4B'
/// title = 'FML Conversion for BiologicallyDerivedProductDispense: R5 to R4B'

uses "http://hl7.org/fhir/StructureDefinition/BiologicallyDerivedProductDispense" alias BiologicallyDerivedProductDispenseR5 as source
uses "http://hl7.org/fhir/4.3/StructureDefinition/BiologicallyDerivedProductDispense" alias BiologicallyDerivedProductDispenseR4B as target

imports "http://hl7.org/fhir/StructureMap/*5to4B"

conceptmap "BiologicallyDerivedProductDispenseStatus" {
  prefix s = "http://hl7.org/fhir/biologicallyderivedproductdispense-status"
  prefix t = "http://hl7.org/fhir/4.3/biologicallyderivedproductdispense-status"

  s:preparation - t:preparation
  s:in-progress - t:in-progress
  s:allocated - t:allocated
  s:issued - t:issued
  s:unfulfilled - t:unfulfilled
  s:returned - t:returned
  s:entered-in-error - t:entered-in-error
  s:unknown - t:unknown
}

group BiologicallyDerivedProductDispense(source src : BiologicallyDerivedProductDispenseR5, target tgt : BiologicallyDerivedProductDispenseR4B) extends DomainResource <<type+>> {
  src.identifier -> tgt.identifier;
  src.basedOn -> tgt.basedOn;
  src.partOf -> tgt.partOf;
  src.status as v -> tgt.status = translate(v, '#BiologicallyDerivedProductDispenseStatus', 'c');
  src.originRelationshipType -> tgt.originRelationshipType;
  src.product -> tgt.product;
  src.patient -> tgt.patient;
  src.matchStatus -> tgt.matchStatus;
  src.performer as s -> tgt.performer as t then BiologicallyDerivedProductDispensePerformer(s,t);
  src.location -> tgt.location;
  src.quantity -> tgt.quantity;
  src.preparedDate -> tgt.preparedDate;
  src.whenHandedOver -> tgt.whenHandedOver;
  src.destination -> tgt.destination;
  src.note -> tgt.note;
  src.usageInstruction -> tgt.usageInstruction;
}

group BiologicallyDerivedProductDispensePerformer(source src, target tgt) extends BackboneElement {
  src.function -> tgt.function;
  src.actor -> tgt.actor;
}

