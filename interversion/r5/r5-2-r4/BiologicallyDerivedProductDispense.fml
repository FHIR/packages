/// url = 'http://hl7.org/fhir/StructureMap/BiologicallyDerivedProductDispense5to4'
/// name = 'BiologicallyDerivedProductDispense5to4'
/// title = 'FML Conversion for BiologicallyDerivedProductDispense: R5 to R4'

uses "http://hl7.org/fhir/StructureDefinition/BiologicallyDerivedProductDispense" alias BiologicallyDerivedProductDispenseR5 as source
uses "http://hl7.org/fhir/4.0/StructureDefinition/BiologicallyDerivedProductDispense" alias AccountR4 as target

imports "http://hl7.org/fhir/StructureMap/*5to4

group BiologicallyDerivedProductDispense(source src : BiologicallyDerivedProductDispenseR5, target tgt : BiologicallyDerivedProductDispenseR4) extends DomainResource <<type+>> {
  src.identifier -> tgt.identifier;
  src.basedOn -> tgt.basedOn;
  src.partOf -> tgt.partOf;
  src.status -> tgt.status;
  src.originRelationshipType -> tgt.originRelationshipType;
  src.product -> tgt.product;
  src.subject -> tgt.subject;
  src.matchStatus -> tgt.matchStatus;
  src.performer as s -> tgt.performer as t then BiologicallyDerivedProductDispensePerformer(s,t);
  src.location -> tgt.location;
  src.quantity -> tgt.quantity;
  src.preparedDate -> tgt.preparedDate;
  src.whenHandedOver -> tgt.whenHandedOver;
  src.destination -> tgt.destination;
  src.note -> tgt.note;
  src.usageInstruction -> tgt.usageInstruction;
}

group BiologicallyDerivedProductDispensePerformer(source src : null, target tgt : null) extends DomainResource <<type+>> {
  src.function -> tgt.function;
  src.actor -> tgt.actor;
}

