<?xml version="1.0" encoding="UTF-8"?><CodeSystem xmlns="http://hl7.org/fhir"><id value="v2-0871"/><meta><profile value="http://hl7.org/fhir/StructureDefinition/shareablecodesystem"/></meta><language value="en"/><extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status"><valueCode value="external"/></extension><extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-fmm"><valueInteger value="0"/></extension><url value="http://terminology.hl7.org/CodeSystem/v2-0871"/><identifier><system value="urn:ietf:rfc:3986"/><value value="urn:oid:2.16.840.1.113883.18.410"/></identifier><version value="2.9"/><name value="v2.0871"/><title value="v2 Supply Risk Codes"/><status value="active"/><experimental value="false"/><publisher value="HL7, Inc"/><contact><telecom><system value="url"/><value value="http://hl7.org"/></telecom></contact><description value="FHIR Value set/code system definition for HL7 v2 table 0871 ( Supply Risk Codes)"/><content value="complete"/><concept><extension url="http://hl7.org/fhir/StructureDefinition/codesystem-concept-comments"><valueString value="Material is corrosive and may cause severe injury to skin, mucous membranes and eyes. Avoid any unprotected contact."><extension url="http://hl7.org/fhir/StructureDefinition/translation"><extension url="lang"><valueCode value="nl"/></extension><extension url="content"><valueString value="Materiaal is corrosief en kan ernstige verwondingen aan de huid, slijmvliezen en ogen veroorzaken. Vermijd onbeschermd contact."/></extension></extension></valueString></extension><code value="COR"/><display value="Corrosive"/><designation><language value="nl"/><use><system value="http://terminology.hl7.org/CodeSystem/designation-usage"/><code value="display"/></use><value value="Corrosief"/></designation></concept><concept><extension url="http://hl7.org/fhir/StructureDefinition/codesystem-concept-comments"><valueString value="Material is an explosive mixture.  Keep away from fire, sparks, and heat."><extension url="http://hl7.org/fhir/StructureDefinition/translation"><extension url="lang"><valueCode value="nl"/></extension><extension url="content"><valueString value="Materiaal is explosief mengsel. Houd weg van vuur, vonken en hitte."/></extension></extension></valueString></extension><code value="EXP"/><display value="Explosive"/><designation><language value="nl"/><use><system value="http://terminology.hl7.org/CodeSystem/designation-usage"/><code value="display"/></use><value value="Explosief"/></designation></concept><concept><extension url="http://hl7.org/fhir/StructureDefinition/codesystem-concept-comments"><valueString value="Material is highly flammable and in certain mixtures (with air) may lead to explosions.  Keep away from fire, sparks and excessive heat."><extension url="http://hl7.org/fhir/StructureDefinition/translation"><extension url="lang"><valueCode value="nl"/></extension><extension url="content"><valueString value="Materiaal is licht ontvlambaar en kan in bepaalde mengsels (met lucht) leiden tot explosies. Houd weg van vuur, vonken en warmtebronnen."/></extension></extension></valueString></extension><code value="FLA"/><display value="Flammable"/><designation><language value="nl"/><use><system value="http://terminology.hl7.org/CodeSystem/designation-usage"/><code value="display"/></use><value value="Brandbaar"/></designation></concept><concept><extension url="http://hl7.org/fhir/StructureDefinition/codesystem-concept-comments"><valueString value="Material is solid and sharp (e.g., cannulas.)  Dispose in hard container."><extension url="http://hl7.org/fhir/StructureDefinition/translation"><extension url="lang"><valueCode value="nl"/></extension><extension url="content"><valueString value="Materiaal is hard en scherp (bijv. canules). Verwijder in harde container."/></extension></extension></valueString></extension><code value="INJ"/><display value="Injury Hazard"/><designation><language value="nl"/><use><system value="http://terminology.hl7.org/CodeSystem/designation-usage"/><code value="display"/></use><value value="Letselgevaar"/></designation></concept><concept><extension url="http://hl7.org/fhir/StructureDefinition/codesystem-concept-comments"><valueString value="Material is a source for ionizing radiation and must be handled with special care to avoid injury of those who handle it and to avoid environmental hazards."><extension url="http://hl7.org/fhir/StructureDefinition/translation"><extension url="lang"><valueCode value="nl"/></extension><extension url="content"><valueString value="Materiaal is een bron voor ioniserende straling en moet met uiterste voorzorgen worden gebruikt om letsel te voorkomen van degene die het gebruiken en milieuschade."/></extension></extension></valueString></extension><code value="RAD"/><display value="Radioactive"/><designation><language value="nl"/><use><system value="http://terminology.hl7.org/CodeSystem/designation-usage"/><code value="display"/></use><value value="Radioactief"/></designation></concept><concept><extension url="http://hl7.org/fhir/StructureDefinition/codesystem-concept-comments"><valueString value="Material is toxic to humans and/or animals.  Special care must be taken to avoid incorporation, even of small amounts."><extension url="http://hl7.org/fhir/StructureDefinition/translation"><extension url="lang"><valueCode value="nl"/></extension><extension url="content"><valueString value="Materiaal is toxisch voor mensen en/of dieren. Let specifiek op vermijding van inname, ook van kleinere hoeveelheden."/></extension></extension></valueString></extension><code value="TOX"/><display value="Toxic"/><designation><language value="nl"/><use><system value="http://terminology.hl7.org/CodeSystem/designation-usage"/><code value="display"/></use><value value="Toxisch"/></designation></concept><concept><extension url="http://hl7.org/fhir/StructureDefinition/codesystem-concept-comments"><valueString value="Material hazard level is unknown."><extension url="http://hl7.org/fhir/StructureDefinition/translation"><extension url="lang"><valueCode value="nl"/></extension><extension url="content"><valueString value="Material hazard level is unknown."/></extension></extension></valueString></extension><code value="UNK"/><display value="Unknown"/><designation><language value="nl"/><use><system value="http://terminology.hl7.org/CodeSystem/designation-usage"/><code value="display"/></use><value value="Onbekend"/></designation></concept></CodeSystem>